#include "game.h"
#include "keyInterface.h" //Some file to manage keypresses

typedef struct game
{
  int score;
  snakePart* snake;
} game;

void tick()
{
  char movement = getChar() // get the currently pressed char from the keyInterface
  int xPos = 0;
  int yPos = 0;
  if(movement == 'a')
    xPos = -1;
  else if(movement == 'd')
    xPos = 1;
  else if(movement == 'w')
    yPos = -1;
  else if(movement == 's')
    yPos = 1;

}
